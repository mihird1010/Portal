  <script type="text/javascript" src='js/jquery-2.2.1.min.js' ></script>
  <script type="text/javascript" src='js/materialize.min.js'></script>
  <script type="text/javascript">
  $('#scheme_data').submit(function(e){
        e.preventDefault();
        $('#scheme_data input').blur();
        submit_scheme_data($('#scheme_data'),rederction);
        return false;
    });

  function submit_scheme_data(form,callback){
	
	$.ajax({
	type: 'POST',
	data: form.serialize(),
	url: '/getscheme/',
	success: function(response){
		callback(response);
	}
	});
}
function redirection(data){
window.location=window.location.origin+"/available_scheme.html?schemes_data="+JSON.stringify(data);
}

  </script>

